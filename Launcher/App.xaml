<Application
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:System="clr-namespace:System;assembly=mscorlib"
             x:Class="Launcher.App"
             StartupUri="MainWindow.xaml" Startup="StartUp">
	<Application.Resources>
		<Style x:Key="RefreshButtonStyle" TargetType="{x:Type Button}">
			<Setter Property="FontFamily" Value="Wingdings 3" />
			<Setter Property="FontSize" Value="16" />
			<Setter Property="Content" Value="P" />
		</Style>
		<FontFamily x:Key="SelectDeselectButtonFontFamily">Wingdings</FontFamily>
		<System:String x:Key="SelectButtonIcon">&#x006F;</System:String>
		<System:String x:Key="DeselectButtonIcon">&#x00FE;</System:String>
		<Image x:Key="OpenDocumentIcon" x:Shared="False"  Source="Resources/folder-open-document.png" />
		<Style x:Key="SelectFileButtonStyle" TargetType="{x:Type Button}">
			<Setter Property="ContentTemplate">
				<Setter.Value>
					<DataTemplate>
						<Grid>
							<StaticResource ResourceKey="OpenDocumentIcon" />
						</Grid>
					</DataTemplate>
				</Setter.Value>
			</Setter>
		</Style>
	</Application.Resources>
</Application>