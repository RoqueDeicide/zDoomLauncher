<Window x:Class="Launcher.CommandLineWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:ui="http://schemas.modernwpf.com/2019"
        Title="Test Command Line" Height="424" Width="592"
        ui:WindowHelper.UseModernWindowStyle="True"
        ui:ThemeManager.IsThemeAware="True">
	<Grid>
		<TextBox x:Name="CommandLineTextBox" Margin="10,10,10,47" TextWrapping="Wrap" Focusable="False" />
		<RichTextBox x:Name="CharacterCountTextBox" Height="32" Margin="10,0,10,10" VerticalAlignment="Bottom" Focusable="False" VerticalContentAlignment="Center"/>
	</Grid>
</Window>